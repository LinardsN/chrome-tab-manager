{
  "manifest_version": 2,
  "name": "Quick Tab Organizer",
  "version": "1.0",
  "description": "A lightweight Chrome extension to manage and organize open tabs.",
  "permissions": ["tabs"],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
